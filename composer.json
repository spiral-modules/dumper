{
    "name": "spiral/dumper",
    "type": "library",
    "description": "Dumper for PHP variables based on Symfony VarDumper for Spiral Framework and RoadRunner",
    "homepage": "https://spiral.dev",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/dumper",
        "docs": "https://spiral.dev/docs",
        "forum": "https://forum.spiral.dev",
        "chat": "https://discord.gg/V6EK4he"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "Pavel Buchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "email": "alexey.gagarin@spiralscout.com"
        },
        {
            "name": "Maksim Smakouz (msmakouz)",
            "email": "maksim.smakouz@spiralscout.com"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spiral"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "buggregator/trap": "^1.8",
        "spiral/boot": "^3.7",
        "symfony/var-dumper": "^6.1 || ^7.0"
    },
    "require-dev": {
        "vimeo/psalm": "^5.14",
        "psr/http-server-middleware": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Debug\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
