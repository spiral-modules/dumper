{
  "name": "spiral/dumper",
  "type": "library",
  "description": "Dumper for PHP variables based on Symfony VarDumper for Spiral Framework and RoadRunner",
  "license": "MIT",
  "homepage": "https://spiral.dev",
  "support": {
    "issues": "https://github.com/spiral/framework/issues",
    "source": "https://github.com/spiral/dumper"
  },
  "authors": [
    {
      "name": "Anton Titov (wolfy-j)",
      "email": "wolfy-j@spiralscout.com"
    },
    {
      "name": "Pavel Buchnev (butschster)",
      "email": "pavel.buchnev@spiralscout.com"
    }
  ],
  "require": {
    "php": ">= 8.1",
    "spiral/boot": "^3.7",
    "symfony/var-dumper": "^6.1 || ^7.0"
  },
  "require-dev": {
    "vimeo/psalm": "^5.14",
    "psr/http-server-middleware": "^1.0",
    "psr/http-factory": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Debug\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
